page{
  background: rgb(240, 244, 251)
}
map{
  width: 100%;
  height: 345rpx
}
.form{
  position: relative;
  display: block;
  width: 682rpx;
  padding-bottom: 175rpx;
  background: white;
  margin: -90rpx auto;
  z-index: 1000
}
.form01{
  padding-top: 20rpx;
  text-align: center;
  font-size: 30rpx
}
.form02{
  text-align: center;
  font-size: 24rpx;
   padding-bottom: 30rpx
}
.form-item{
  margin-top: 40rpx;
  display: flex;
  width: 100%;
  justify-content: center
}
.form-item image{
  width: 45rpx;
  height: 50rpx
}
.form-item view{
  width: 540rpx;
  height: 50rpx;
  font-size: 30rpx;
  border-bottom: 1px solid rgb(226, 239, 253);
  margin-left: 33rpx;
  padding-bottom: 20rpx
  
}
.lan{
  color: #005aab
}
.form-item:last-of-type{
  margin: 15rpx
}
.form-item:last-of-type view{
  border: 0!important;
  color: #6d6d6d;
  font-size: 21rpx
}