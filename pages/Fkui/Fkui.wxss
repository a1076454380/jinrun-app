page{
  background: rgb(240, 244, 251)
}
.top,.top image{
  width: 100%;
  height: 296rpx;
  color: white;
  font-size: 36rpx
}
.top{
  position: relative
}
.top image{
  position: absolute
}
.top navigator{
  position: relative;
  z-index: 999;
  text-align: center;
  padding-top: 90rpx
}
form{
  display: block;
  margin: 0 auto;
  width: 682rpx;
  height: auto;
  padding-top: 34rpx
}
input{
  width: 100%;
  height: 84rpx;
  border: 1px solid rgb(221, 221, 221);
  border-radius: 6rpx;
  background: white;
  padding-left: 24rpx;
  box-sizing: border-box;
  font-size: 29rpx;
  color: #6d6d6d
}
textarea{
  width: 100%;
  height: 315rpx;
   border: 1px solid rgb(221, 221, 221);
  border-radius: 6rpx;
  background: white;
  padding: 24rpx;
   box-sizing: border-box;
  font-size: 29rpx;
  color: #6d6d6d;
  margin-top: 25rpx
}
.Txing{
  font-size: 24rpx;
  color: #b1b1b1;
  line-height: 36rpx;
  padding-top: 25rpx
}
button{
  width: 268rpx;
  height: 85rpx;
  background: rgb(0,90,171);
  color: white;
  font-size: 36rpx;
  margin-top: 50rpx;
  line-height: 85rpx;
  border-radius: 60rpx
}